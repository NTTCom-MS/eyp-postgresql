{
  "name": "eyp-postgresql",
  "version": "0.5.5",
  "author": "eyp",
  "summary": "postgresql and pgbounce installation, configuration and management",
  "license": "Apache-2.0",
  "source": "https://github.com/NTTCom-MS/eyp-postgresql",
